        <p><i>Yes, we are open!</i></p>
    <% } else { %>
        <p><i>Sorry, we are closed</i></p>
    <% } %>
    <p><%= RESTAURANT.name %></p>
    <p><%= RESTAURANT.address %></p>
    <p><%= RESTAURANT.phone %></p>
</body>
</html>
